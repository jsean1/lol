local circlesFolder = workspace.Circles
local player = game.Players.LocalPlayer

getgenv().on = false

while getgenv().on do
    for _, circle in ipairs(circlesFolder:GetChildren()) do
        if circle:IsA("BasePart") then
            player.Character:SetPrimaryPartCFrame(CFrame.new(circle.Position + Vector3.new(0, 1, 0)))

            
            wait(0.1)
        end
    end
    wait(0.2)
end
